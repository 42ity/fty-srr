[Unit]
Description=fty_srr service
After=malamute.service network.target
Requires=malamute.service network.target
Before=fty_config.service
PartOf=bios.target

[Service]
Type=simple
User=bios
Environment="prefix=@prefix@"
Environment='SYSTEMD_UNIT_FULLNAME=%n'
ExecStart=@prefix@/bin/fty_srr @sysconfdir@/@PACKAGE@/fty_srr.cfg
Restart=always

[Install]
WantedBy=bios.target
